{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "bbp-core/forum-ajax",
    "version": "0.1.0",
    "title": "BBPC Forums with Ajax Filters",
    "category": "bbp-core",
    "icon": "admin-generic",
    "description": "Displays a list of bbPress topics with filtering options.",
    "attributes": {
        "ppp2": {
            "type": "number",
            "default": 9
        },
        "order": {
            "type": "string",
            "default": "ASC"
        },
        "filter_btns": {
            "type": "boolean",
            "default": true
        },
        "forum_title_color": {
            "type": "string"
        },
        "forum_title_hover_color": {
            "type": "string"
        },
        "forum_meta_color": {
            "type": "string"
        },
        "parent_forum_color": {
            "type": "string"
        },
        "parent_forum_color_hover": {
            "type": "string"
        }
    },
    "supports": {
        "html": false,
        "typography": {
            "fontSize": true,
            "lineHeight": true,
            "__experimentalFontFamily": true,
            "__experimentalFontWeight": true,
            "__experimentalFontStyle": true,
            "__experimentalTextTransform": true,
            "__experimentalTextDecoration": true,
            "__experimentalLetterSpacing": true,
            "__experimentalDefaultControls": {
                "fontSize": true
            }
        }
    },
    "textdomain": "bbp-core",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./index.css"
}